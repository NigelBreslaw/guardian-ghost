    "lint:fix": "biome check --apply ./src",
    "preview": "vite preview",
    "type-check": "tsc",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "solid-devtools": "0.30.1",
    "typescript": "5.5.3",
    "vite": "5.3.2",
    "vite-plugin-solid": "2.10.2"
  },
  "dependencies": {
    "solid-js": "1.8.18"
  }
}
