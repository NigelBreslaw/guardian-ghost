{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "noUncheckedIndexedAccess": true,
    "noErrorTruncation": true,
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "strict": true
  },
  "include": [
    "App.tsx",
    "**/*.ts",
    "**/*.tsx"
  ]
}
