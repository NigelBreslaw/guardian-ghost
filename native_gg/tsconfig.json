{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "noUncheckedIndexedAccess": true,
    "noErrorTruncation": true,
    "baseUrl": "app",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "strict": true
  },
  "include": [
    "App.tsx",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
