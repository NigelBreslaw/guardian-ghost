{
  "name": "@gg-monorepo/starlight",
  "version": "0.0.1",
  "dependencies": {
    "@astrojs/starlight": "0.15.3",
    "astro": "4.1.2",
    "rehype": "13.0.1",
    "sharp": "0.33.2"
  },
  "devDependencies": {
    "@astrojs/check": "0.4.1",
    "@types/hast": "3.0.3",
    "@vitest/coverage-v8": "1.1.3",
    "typescript": "5.3.3",
    "vitest": "1.1.3",
    "hast-util-from-html": "2.0.1",
    "hast-util-to-string": "3.0.0",
    "hastscript": "8.0.0",
    "unist-util-visit": "5.0.0"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "coverage": "vitest run --coverage",
